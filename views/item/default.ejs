<div style="display: flex; justify-content: space-between;">
  <h1>Inventory</h1>
  <a href="/item/new">âž• New Item</a>

</div>
<div class="items-grid" style="padding: 0px 14px;">
  <p>Name</p>
  <p>Price</p>
  <p>Available</p>
  <p style="margin-left: auto;">Actions</p>
</div>
<ul class="list">
  <% items.forEach( item => { %>
  <li class="items-grid">
    <a href="/item/<%= item.id %>"><%= item.name %></a>
    <p><%= item.price %></p>
    <p><%= item.quantity %></p>
    <div style="margin-left: auto;">
      <a href="/item/edit/<%= item.id %>">Edit</a>
      <a href="/item/delete/<%= item.id %>">Delete</a>
    </div>
  </li>
  <% })%>
</ul>
